{
	"name": "Revared",
	
	"tree": {
		"$className": "DataModel",
		"workspace": {
			"$className": "Workspace",
			
			"Revared": {
				"$className": "ModuleScript",
				"$path": "Revared/Main",


				"Services": {
					"$className": "Folder",

					"Server": {
						"$className": "ModuleScript",
						"$path": "Revared/Services/Server",
						
						"Events": {
							"$className": "Folder",
	
							"Out": { "$className": "RemoteEvent" },
							"Post": { "$className": "RemoteEvent" },
							"Request": { "$className": "RemoteEvent" }
						}
					},
	
	
					"Trigger": {
						"$className": "ModuleScript",
						"$path": "Revared/Services/Trigger",
	
						"TriggerPlayerHandler": {
							"$className": "Script",
	
							"$properties": {
								"RunContext": "Client"
							},
							
							"$path": "Revared/Services/Trigger/TriggerPlayerHandler"
						},
	
						"TriggerHandler": {
							"$className": "Script",
							
							"$properties": {
								"RunContext": "Client"
							},
							
							"$path": "Revared/Services/Trigger/TriggerHandler"
						}
					},
	
					"Signal": {
						"$className": "ModuleScript",
						"$path": "Revared/Services/Signal"
					}
				}
			}
		}
	}
}
