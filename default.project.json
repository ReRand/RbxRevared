{
	"name": "Revared",
	
	"tree": {
		"$className": "DataModel",
		"workspace": {
			"$className": "Workspace",
			
			"Revared": {
				"$className": "ModuleScript",
				"$path": "Revared/Main",
				
				"Server": {
					"$className": "ModuleScript",
					"$path": "Revared/Server",
					
					"Events": {
						"$className": "Folder",

						"Out": { "$className": "RemoteEvent" },
						"Post": { "$className": "RemoteEvent" },
						"Request": { "$className": "RemoteEvent" }
					}
				},


				"Trigger": {
					"$className": "ModuleScript",
					"$path": "Revared/Trigger",

					"TriggerPlayerHandler": {
						"$className": "Script",

						"$properties": {
							"RunContext": "Client"
						},
						
						"$path": "Revared/Trigger/TriggerPlayerHandler"
					},

					"TriggerHandler": {
						"$className": "Script",
						
						"$properties": {
							"RunContext": "Client"
						},
						
						"$path": "Revared/Trigger/TriggerHandler"
					}
				},

				"Signal": {
					"$className": "ModuleScript",
					"$path": "Revared/Signal"
				}
			}
		}
	}
}
