		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			
			"Revared": {
				"$className": "ModuleScript",
				"$path": "Revared/Main",
				
				"Server": {
					"$className": "ModuleScript",
					"$path": "Revared/Server",
					
					"Events": {
						"$className": "Folder",

						"Out": { "$className": "RemoteEvent" },
						"Post": { "$className": "RemoteEvent" },
						"Request": { "$className": "RemoteEvent" }
					}
				},


				"Trigger": {
					"$className": "ModuleScript",
					"$path": "Revared/Trigger",

					"TriggerPlayerHandler": {
						"$className": "Script",
						"$runContext": "Client",
						
						"$path": "Revared/Trigger/TriggerPlayerHandler"
					},

					"TriggerHandler": {
						"$className": "Script",
						"$runContext": "Client",
						
						"$path": "Revared/Trigger/TriggerHandler"
					}
				},

				"Signal": {
					"$className": "ModuleScript",
					"$path": "Revared/Signal"
				}
			}
		}
	}
